{
  "models": [{
    "model": "qwen3:30b",
    "provider": "ollama"
  }],
  "mcpServers": {
    "local-server": {
      "command": "python",
      "args": ["mcp-server/main.py"]
    },
    "filesystem": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "E:\\Documents\\GitHub\\MCP-Agent"
      ]
    },
    "brave-search": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "brave-search-mcp"
      ]
    },
    "puppeteer": {
      "command": "npx.cmd",
      "args": ["-y", "puppeteer-mcp-server"],
      "env": {}
    },
    "notionApi": {
      "command": "npx.cmd",
      "args": ["-y", "@notionhq/notion-mcp-server"]
    },
    "sequential-thinking": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}